// Generates the first 100 primes

int n := 2;

for int numPrimes := 0; numPrimes < 100; n++ {
    bool isPrime := true;
    for int divisor := 2; divisor < n/2 and isPrime; divisor++ {
        if divisor | n {
            isPrime := false;
        }
    }

    if isPrime {
        numPrimes++;
        print(n);
    }
}
